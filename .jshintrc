{
  "bitwise": true,
  "curly": false,
  "eqeqeq": true,
  "forin": true,
  "freeze": true,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonbsp": true,
  "nonew": false,
  "quotmark": "single",
  "undef": true,

  "maxcomplexity": 10,
  "maxdepth": 4,
  "maxlen": 120,
  "maxparams": 5,
  "maxstatements": 25,

  "eqnull": true,
  "esnext": true,
  "no-unused-expressions": false,

  "node": true,
  "globals": {
        "describe": false,
        "xdescribe": false,
        "ddescribe": false,
        "it": false,
        "xit": false,
        "beforeEach": false,
        "afterEach": false,
        "before": false,
        "after": false
    }
}
